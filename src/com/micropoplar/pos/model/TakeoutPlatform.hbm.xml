<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.micropoplar.pos.model">
	<class lazy="false" name="com.micropoplar.pos.model.TakeoutPlatform" table="TAKEOUT_PLATFORM">

		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property column="NAME" length="30" name="name" type="java.lang.String"/>
		<property column="ENABLED" name="enabled" type="java.lang.Boolean" />
		<property column="DISCOUNT" length="52" name="discount" type="java.lang.Double" />

	</class>
</hibernate-mapping>